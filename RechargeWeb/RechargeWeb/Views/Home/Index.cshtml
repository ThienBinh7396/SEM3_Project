@model Tuple<RechargeWeb.Models.ContainerData>
@using RechargeWeb.Models;
@using System.Text.RegularExpressions;
@{
    Layout = "~/Views/_ViewHomeStart.cshtml";
    var user = Session["user"] as customer;
    var container = Model.Item1;

    if (Session["cart"] == null)
    {
        Session["cart"] = new List<Cart>();
    }
}
@section Main{
    <style>
        .service_body {
            height: 210px;
            overflow-y: auto;
            padding: 20px;
        }

        .tb-buy-service {
            cursor: pointer;
            user-select: none;
        }
    </style>

    <!-- header-area start -->
    <header class="header-area header-area-bg" id="home">
        <div class="header-slider">
            <div class="single-header-slide-item">
                <div class="container">
                    <div class="row">
                        <div class="col-md-8 " id="container_login">
                            <h1>Recharge from anywhere in the world. Anytime.</h1>
                            <p id="sign_in">
                                Your online mobile recharge transaction is completely guaranteed and secure.Earn reward points &amp; get bonus
                                anytime, anywhere.
                            </p>
                            @if (user == null)
                            {
                                <div class="header-btn-group">
                                    <input id="ip_phone" type="number" placeholder="Enter your phone">
                                    <input id="ip_password" type="password" placeholder="Enter password">
                                    <input type="submit" value="Login" id="btn_login">
                                </div>
                            }
                            else
                            {
                                <div class="header-btn-group">
                                    <h3>Hello <strong>@user.phone_number</strong></h3>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header><!-- end header-area -->
