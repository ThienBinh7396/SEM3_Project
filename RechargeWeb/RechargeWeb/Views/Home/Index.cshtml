    <!-- choose your plan area start -->
    <section class="choose-plan choose-plan-bg" id="plan" style="padding-bottom: 56px;">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="section-title">
                        <h2>
                            Choose
                            <span>Your Service</span>
                        </h2>
                    </div>
                </div>
            </div>
            @helper SplitText(string text)
            {
                var arr = Regex.Split(text, @"[\.]+");
                foreach (var i in arr)
                {
                    <li>@i.ToString().</li>
                }
            }
            <div class="row">
                @foreach (var item in container.services)
                {
                    <div class="col-md-4 col-sm-6">
                        <div class="single-price-plan">

                            <div class="header" s-id="@item.id" style="padding-top: 39px">
                                <span class="pname" style="color: #2141ba;font-size: 17px;">@item.name</span>
                                <h2>$<span class="service_price">@String.Format("{0:0.##}", item.price)</span> </h2>
                            </div>
                            <div class="body" style="padding: 0 24px 0px; height: 240px; overflow-y: auto;">
                                @Html.Raw(item.description)
                            </div>
                            <p class="boxed-btn tb-buy-service js_clear_default" style="margin-top: 26px;margin-bottom: 26px;">Buy service</p>
                        </div>
                    </div>
                }
            </div>
            @helper GetNameMobileNetWork(ContainerData container)
            {
                foreach (var item in container.mobile_Networks)
                {
                    if (item.id == (Session["user"] as customer).id_network)
                    {
                        <a href="@Url.Action("service", "Home")" class="boxed-btn blank">See more services from @item.name</a>
                    }
                }

            }
            <div class="row">
                <div class="col">
                    <div class="reffer-section" style="padding: 104px 0 0;border-radius: 25px; width: fit-content; margin: auto">
                        <div class="single-reffer-box" id="cover-button-see-more" style="padding: 0; border-radius: 25px;">

                            @if (user == null)
                            {
                                <a href="#" class="boxed-btn blank">See more services</a>
                            }
                            else
                            {
                                @GetNameMobileNetWork(container)
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <style>
        #cover-button-see-more:after {
            border-radius: 0%;
        }

        #cover-button-see-more a {
            border-radius: 0;
            padding: 16px 84px;
            font-size: 22px;
        }
    </style>
    <!-- choose your plan area end -->
    <!-- reffer section start -->
    <!-- reffer section end -->
    <!-- award wining area start -->
    <section class="award-wining-area" id="award">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-3">
                    <div class="section-title">
                        <h2>Award winning prepaid mobile</h2>
                    </div>
                </div>
            </div>
            <div class="row">
